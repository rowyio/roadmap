    "daisyui": "^2.19.0",
    "dayjs": "^1.11.3",
    "feedbackfin": "^1.1.0",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix-utils": "^3.3.0",
    "theme-change": "^2.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
